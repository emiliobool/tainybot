{
  "name": "tainybot",
  "version": "0.1.16",
  "main": "dist-electron/main/index.js",
  "description": "GPT powered chatbot for twitch.",
  "author": "Emilio Bool <contact@emiliobool.com>",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "build-windows": "vite build && electron-builder build --windows",
    "build-ts": "vue-tsc --noEmit && vite build && electron-builder",
    "publish": "vite build && electron-builder build --windows --publish always",
    "upload-latest": "node upload-latest.js",
    "preview": "vite preview",
    "postinstall": "install-app-deps"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "aws-sdk": "^2.1343.0",
    "electron": "^26.2.4",
    "electron-builder": "^23.6.0",
    "postcss": "^8.4.21",
    "typescript": "^5.0.3",
    "vite": "^5.0.4",
    "vite-plugin-electron": "^0.14.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.7"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.12",
    "@heroicons/vue": "^2.0.17",
    "@tailwindcss/forms": "^0.5.3",
    "electron-log": "^4.4.8",
    "electron-updater": "^6.1.4",
    "luxon": "^3.3.0",
    "node-fetch": "^3.3.1",
    "nunjucks": "^3.2.3",
    "openai": "^4.11.0",
    "pinia": "^2.0.33",
    "sqlite3": "^5.1.4",
    "tailwindcss": "^3.2.7",
    "tiktoken": "^1.0.10",
    "tmi.js": "1.9.0-pre.1"
  }
}
